<!DOCTYPE html>
<html>
<head>
  <nav class="navbar navbar-default">
	<div class="container-fluid">
	  <div class="navbar-header">
	    <a class="navbar-brand" href="<%= root_path %>">
	      Tracking System
	    </a>
	    <ul class="nav navbar-nav">
	        <li class="active">
	        	<%= link_to 'Analytics', analytics_path %>
	        </li>
	      </ul>
	  </div>
	</div>
  </nav>

  <%= stylesheet_link_tag    'application', media: 'all', 'data-turbolinks-track' => true %>
  <%= javascript_include_tag 'application', 'data-turbolinks-track' => true %>
  <%= csrf_meta_tags %>
</head>
<body>
	<% if notice %>
		<div class="alert alert-success col-md-8 col-md-offset-2" style="margin-top: 10px;" role="alert">
		  <%= notice %>
		</div>
	<% end %>
	<% if flash[:error] %>
		<div class="alert alert-warning col-md-8 col-md-offset-2" style="margin-top: 10px;" role="alert">
		  <%= flash[:error] %>
		</div>
	<% end %>

	<%= yield %>

</body>
</html>
